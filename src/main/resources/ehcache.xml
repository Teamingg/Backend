<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.ehcache.org/v3"
        xsi:schemaLocation="http://www.ehcache.org/v3 http://www.ehcache.org/schema/ehcache-core.xsd">
    <cache alias="mentoringTeamParticipationCache">
        <key-type>java.lang.Long</key-type> <!-- 키: ID -->
        <value-type>com.project.Teaming.domain.mentoring.dto.response.TeamParticipationResponse</value-type>
        <expiry>
            <ttl unit="minutes">60</ttl> <!-- 캐시 TTL: 60분 -->
        </expiry>
        <heap unit="entries">1000</heap> <!-- 최대 1000개 엔트리 -->
    </cache>
</config>
